{
	"name": "@neovici/cosmoz-collapse",
	"version": "1.0.0",
	"description": "A simple dropdown web component",
	"keywords": [
		"lit-html",
		"web-components"
	],
	"homepage": "https://github.com/neovici/cosmoz-collapse#readme",
	"bugs": {
		"url": "https://github.com/neovici/cosmoz-collapse/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/neovici/cosmoz-collapse.git"
	},
	"license": "Apache-2.0",
	"author": "",
	"main": "src/index.js",
	"directories": {
		"test": "test"
	},
	"scripts": {
		"lint": "eslint --cache --ext .js .",
		"lint-tsc": "tsc",
		"start": "wds",
		"test": "wtr --coverage",
		"test:watch": "wtr --watch",
		"storybook:build": "build-storybook",
		"storybook:deploy": "storybook-to-ghpages",
		"prepare": "husky install"
	},
	"release": {
		"plugins": [
			"@semantic-release/commit-analyzer",
			"@semantic-release/release-notes-generator",
			"@semantic-release/changelog",
			"@semantic-release/github",
			"@semantic-release/npm",
			"@semantic-release/git"
		],
		"branch": "master",
		"preset": "conventionalcommits"
	},
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"src/*.js"
	],
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"dependencies": {},
	"devDependencies": {
		"@commitlint/cli": "^13.0.0",
		"@commitlint/config-conventional": "^13.0.0",
		"@neovici/eslint-config": "^1.2.0",
		"@semantic-release/changelog": "^6.0.0",
		"@semantic-release/git": "^10.0.0",
		"husky": "^7.0.0",
		"prettier": "^2.5.1",
		"semantic-release": "^18.0.0",
		"typescript": "^4.0.0"
	}
}
